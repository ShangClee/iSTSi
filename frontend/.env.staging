# Staging Environment Configuration
VITE_APP_ENV=staging
VITE_APP_VERSION=1.0.0-staging

# API Configuration
VITE_API_URL=https://staging-api.bitcoin-custody.com
VITE_WS_URL=wss://staging-api.bitcoin-custody.com/ws

# Security Configuration
VITE_ENABLE_CSRF_PROTECTION=true
VITE_ENABLE_REQUEST_SIGNING=false
VITE_MAX_REQUEST_TIMEOUT=15000
VITE_RATE_LIMIT_PER_MINUTE=100

# Feature Flags
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true

# Soroban Network Configuration
VITE_SOROBAN_NETWORK=testnet
VITE_SOROBAN_RPC_URL=https://soroban-testnet.stellar.org
VITE_SOROBAN_NETWORK_PASSPHRASE="Test SDF Network ; September 2015"

# Application Configuration
VITE_APP_NAME="Bitcoin Custody Platform (Staging)"
VITE_APP_DESCRIPTION="Staging environment for Bitcoin custody platform"
VITE_SUPPORT_EMAIL=staging-support@bitcoin-custody.com

# Content Security Policy
VITE_CSP_SCRIPT_SRC="'self' 'unsafe-eval'"
VITE_CSP_STYLE_SRC="'self' 'unsafe-inline'"
VITE_CSP_IMG_SRC="'self' data: https:"
VITE_CSP_CONNECT_SRC="'self' https://staging-api.bitcoin-custody.com wss://staging-api.bitcoin-custody.com http://localhost:8080 ws://localhost:8080"

# Monitoring and Analytics
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Build Configuration
VITE_BUILD_TIMESTAMP=
VITE_GIT_COMMIT_HASH=