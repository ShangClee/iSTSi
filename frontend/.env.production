# Production Environment Configuration
VITE_APP_ENV=production
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_URL=https://api.bitcoin-custody.com
VITE_WS_URL=wss://api.bitcoin-custody.com/ws

# Security Configuration
VITE_ENABLE_CSRF_PROTECTION=true
VITE_ENABLE_REQUEST_SIGNING=true
VITE_MAX_REQUEST_TIMEOUT=10000
VITE_RATE_LIMIT_PER_MINUTE=60

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Soroban Network Configuration
VITE_SOROBAN_NETWORK=mainnet
VITE_SOROBAN_RPC_URL=https://soroban-mainnet.stellar.org
VITE_SOROBAN_NETWORK_PASSPHRASE="Public Global Stellar Network ; September 2015"

# Application Configuration
VITE_APP_NAME="Bitcoin Custody Platform"
VITE_APP_DESCRIPTION="Secure Bitcoin custody with Stellar integration"
VITE_SUPPORT_EMAIL=support@bitcoin-custody.com

# Content Security Policy
VITE_CSP_SCRIPT_SRC="'self'"
VITE_CSP_STYLE_SRC="'self' 'unsafe-inline'"
VITE_CSP_IMG_SRC="'self' data: https:"
VITE_CSP_CONNECT_SRC="'self' https://api.bitcoin-custody.com wss://api.bitcoin-custody.com"

# Monitoring and Analytics
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Build Configuration
VITE_BUILD_TIMESTAMP=
VITE_GIT_COMMIT_HASH=